# pandas package is required
import pandas as pd

# converting csv file to data frame
data_frame = pd.read_csv("exam.log", sep='\t',
                names=['Date','INFO','Age','Height','comments'])
 
data = data_frame
data
total_transaction = data['comments'].value_counts().count()
#data.query("comments == end transaction")
total_transaction


data = data.groupby(['Date','INFO','Age','Height'])['comments'].apply(', '.join).reset_index()

#change column order                           
uniq = data[['Date','INFO','Age','Height','comments']] 
uniq
